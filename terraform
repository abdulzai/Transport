terraform init
terraform apply \
  -var='project_name=otbridge' \
  -var='environment=dev' \
  -var='region=us-east-1' \
  -var='kinesis_stream_name=YOUR_STREAM' \
  -var='kinesis_stream_arn=arn:aws:kinesis:us-east-1:123456789012:stream/YOUR_STREAM' \
  -var='kms_key_arn=arn:aws:kms:us-east-1:123456789012:key/ABC-...'
